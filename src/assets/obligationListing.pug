extends personCenter
block centerContent
    #app
        .my-record-wrapper
            h3.my-title
                span 进行中的消费返利
            .table-wrapper
                .offcial-table
                    ul.tr-th.clearfix
                        li.th.w20 序号
                        li.th.w30 用户
                        li.th.w30 授权额度
                        li.th.w20 编号
                    ul.tr-td.clearfix(v-for="(item,index) in queueList")
                        li.th.w20(v-text="index+1")
                        li.th.w30(v-text="item.name")
                        li.th.w30(v-text="item.debt_money")
                        li.th.w20(v-text="item.debtcode")

                
